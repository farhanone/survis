@incollection{Post2003,
	abstract = {Flow visualization is an attractive topic in data visualization, offering
	great challenges for research. Very large data sets must be processed,
	consisting of multivariate data at large numbers of grid points,
	often arranged in many time steps. Recently, the steadily increasing
	performance of computers again has become a driving force for new
	advances in flow visualization, especially in techniques based on
	texturing, feature extraction, vector field clustering, and topology
	extraction. In this article we present the state of the art in feature-based
	flow visualization techniques. We will present numerous feature extraction
	techniques, categorized according to the type of feature. Next, feature
	tracking and event detection algorithms are discussed, for studying
	the evolution of features in time-dependent data sets. Finally, various
	visualization techniques are demonstrated.},
	address = {Oxford, UK and Boston, USA},
	author = {Frits H. Post},
	booktitle = {Computer Graphics Forum},
	date-added = {2021-01-20 23:45:04 +0100},
	date-modified = {2021-01-20 23:45:04 +0100},
